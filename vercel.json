{
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/pages/(.*)", "dest": "/pages/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
